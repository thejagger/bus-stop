{
  "expo": {
    "name": "Bus Stop",
    "slug": "bus-stop",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.busstop.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.busstop.app"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "bus-stop"
  }
}
